block content

		each post in posts
			div(class="postheader")
				span #{post[postNum]}
			div(class="post")
				div(class="poster-info")
					span #{post[posterUN]}
					span #{post[numofPost]}
				div(class="post-content")
					p #{post[content]}
					div(class="post-footer")
						span #{post[createDate]}
						
						if post[editdate]
							span #{post[editDate]}